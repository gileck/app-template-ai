{
  "name": "Agent(app-template-ai): Bug Investigator",
  "uniqueKey": "app-template:agent:bug-investigator",
  "description": "Runs the bug investigator agent every 10 minutes",
  "script": {
    "path": "github-workflows-agent",
    "args": ["--bug-investigator", "--stream", "--reset"],
    "interpreter": "npm",
    "workingDirectory": "/Users/gileck/Projects/agents-copy/app-template-ai"
  },
  "schedule": {
    "type": "interval",
    "value": "600000"
  },
  "retry": {
    "enabled": true,
    "maxAttempts": 3,
    "backoffType": "exponential",
    "initialDelayMs": 10000,
    "maxDelayMs": 60000
  },
  "timeout": { "ms": 900000 },
  "notifications": {
    "onStart": false,
    "onSuccess": false,
    "onFailure": true
  },
  "options": {
    "enabled": true,
    "allowParallelRuns": false,
    "requiresInternet": true
  },
  "output": {
    "logFile": "/Users/gileck/Projects/app-template-ai/agent-tasks/bug-investigator/runs/output.log",
    "statusFile": "/Users/gileck/Projects/app-template-ai/agent-tasks/bug-investigator/runs/status.json"
  }
}
